import rdchord
from rdkit.Chem import MCS
rc=rdchord.rdchord(GD)
mols = []
n = 0
for rdmol in rdmols:
  n += 1
  m = rc.from_binary(rdmol)
  if m:
    mols.append(m)
  else:
   plpy.error("Error in input rdmol #%d" % n)
   return None
MCSResult = MCS.FindMCS(mols)
return MCSResult.smarts
