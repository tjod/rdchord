import rdchord
rc=rdchord.rdchord(GD)
mol = rc.parse_molblock(mb)
if mol:
 pairdata = list()
 tagorder = 1
 for p in mol.GetPropNames():
  mol.GetProp(p)
  pairdata.append((p, mol.GetProp(p), tagorder))
  tagorder += 1
 return pairdata
else:
 plpy.warning("Error parsing molfile")
 return [None]

